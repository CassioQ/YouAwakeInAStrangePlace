{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "jsx": "react-native",
    "moduleResolution": "bundler",
    "noEmit": true,
    "strict": true,
    "target": "es2017",
    "types": ["react", "react-native"]
  },
  "exclude": ["node_modules"]
}
